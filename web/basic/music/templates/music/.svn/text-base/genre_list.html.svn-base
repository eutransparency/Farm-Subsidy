{% extends "music/base_music.html" %}


{% block title %}Music Genres{% endblock %}
{% block body_class %}{{ block.super }} music_genres{% endblock %}


{% block content_title %}
  <h2>Music Genres</h2>
  {% include "music/_nav.html" %}
{% endblock %}


{% block content %}
  <ul class="link_list">
  {% for genre in object_list %}
    <li><a href="{{ genre.get_absolute_url }}">{{ genre.title }}</a></li>
  {% endfor %}
  </ul>
{% endblock %}