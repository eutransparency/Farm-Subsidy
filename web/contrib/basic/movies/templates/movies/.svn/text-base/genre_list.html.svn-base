{% extends "movies/base_movies.html" %}


{% block title %}Movie Genres{% endblock %}
{% block body_class %}{{ block.super }} movie_genres{% endblock %}


{% block content_title %}
  <h2>Movie Genres</h2>
  {% include "movies/_nav.html" %}
{% endblock %}


{% block content %}
  <ul class="link_list">
  {% for genre in object_list %}
    <li><a href="{{ genre.get_absolute_url }}">{{ genre.title }}</a></li>
  {% endfor %}
  </ul>
{% endblock %}