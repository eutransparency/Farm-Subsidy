{% extends "media/base_photos.html" %}


{% block title %}Photos{% endblock %}


{% block content_title %}
  <h2>Photos</h2>
{% endblock %}


{% block content %}
  <ul class="photo_list">
  {% for photo in object_list %}
    <li><a href="{{ photo.get_absolute_url }}">{{ photo.title }}</a></li>
  {% endfor %}
  </ul>
{% endblock %}